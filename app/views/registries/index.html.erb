<p id="notice"><%= notice %></p>

<h1>Registries</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Organ</th>
      <th>County</th>
      <th>Last update</th>
      <th>Holder</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registries.each do |registry| %>
      <tr>
        <td><%= registry.code %></td>
        <td><%= registry.organ %></td>
        <td><%= registry.county %></td>
        <td><%= registry.last_update %></td>
        <td><%= registry.holder %></td>
        <td><%= registry.email %></td>
        <td><%= registry.address %></td>
        <td><%= link_to 'Show', registry %></td>
        <td><%= link_to 'Edit', edit_registry_path(registry) %></td>
        <td><%= link_to 'Destroy', registry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Registry', new_registry_path %>
