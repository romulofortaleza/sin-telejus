<p id="notice"><%= notice %></p>

<h1>Address Organs</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Organ</th>
      <th>Last update</th>
      <th>Type</th>
      <th>Name</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @address_organs.each do |address_organ| %>
      <tr>
        <td><%= address_organ.code %></td>
        <td><%= address_organ.organ %></td>
        <td><%= address_organ.last_update %></td>
        <td><%= address_organ.type %></td>
        <td><%= address_organ.name %></td>
        <td><%= address_organ.address %></td>
        <td><%= address_organ.phone %></td>
        <td><%= address_organ.email %></td>
        <td><%= link_to 'Show', address_organ %></td>
        <td><%= link_to 'Edit', edit_address_organ_path(address_organ) %></td>
        <td><%= link_to 'Destroy', address_organ, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Address Organ', new_address_organ_path %>
