<div class="wrapper wrapper-content animated fadeInRight">
  <div class="row">
    <div class="col-lg-12">
      <div class="ibox float-e-margins">
        <div class="ibox-content-scaffold">
        <center style="color:#49667d;"><h1> <%=  @room.room %></h1></center>
        <div class="hr-line-dashed"></div>
            <form class="form-horizontal">
              <div class="form-group"><label style="color:#49667d;" class="col-sm-2 control-label">Localização:</label>
                <div class="col-sm-10">
                  <p style="color:#565555;" class="form-control"> 
                    <%= @room.locate %>
                  </p>
                 </div>
                </div>                  
                <div class="form-group"><label style="color:#49667d;" class="col-sm-2 control-label">Última Atualização:</label>
                <div class="col-sm-10">
                  <p style="color:#565555;" class="form-control">
                    <%= @room.last_update %>
                  </p>
                  </div>
                </div>                  
                  <div class="form-group"><label style="color:#49667d;" class="col-sm-2 control-label">Telefone(s):</label>
                <div class="col-sm-10">
                  <p style="color:#565555;" class="form-control">
                    <%= @room.phones %>
                  </p>
                  </div>
                </div>
                <div class="hr-line-dashed">
              </div>                    
              <div class="form-group">
                  <div class="col-sm-4">
                      <%= link_to 'Voltar', rooms_path, :class => 'btn btn-primary' %>                           
                  </div>
              </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>  

